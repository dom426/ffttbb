<Application x:Class="ffTTbb.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ffTTbb"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="FirstBrush" Color="#48468C" />
            <SolidColorBrush x:Key="SecondBrush" Color="#2E3559" />
            <SolidColorBrush x:Key="ThirdBrush" Color="#1F3140" />
            <SolidColorBrush x:Key="FourthBrush" Color="#0B0D0B" />
            <SolidColorBrush x:Key="FifthBrush" Color="#8C7D61" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
